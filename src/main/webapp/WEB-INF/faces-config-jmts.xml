<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
      
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobCostEstimateDialog</from-outcome>
            <to-view-id>/dashboard/job/jobCostEstimateDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>priceListDialog</from-outcome>
            <to-view-id>/job/finance/priceListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>proformaInvoiceDialog</from-outcome>
            <to-view-id>/job/finance/proformaInvoiceDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>proformaInvoicesDialog</from-outcome>
            <to-view-id>/dashboard/job/proformaInvoicesDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobCostingDialog</from-outcome>
            <to-view-id>/job/finance/jobCostingDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>   
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>statusNoteDialog</from-outcome>
            <to-view-id>/job/statusNoteDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobSampleDialog</from-outcome>
            <to-view-id>/job/sample/jobSampleDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>      
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobSampleDeleteConfirmDialog</from-outcome>
            <to-view-id>/job/sample/jobSampleDeleteConfirmDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>         
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>clientCreditStatusDialog</from-outcome>
            <to-view-id>/job/finance/accpac/clientCreditStatusDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cashPaymentDialog</from-outcome>
            <to-view-id>/job/finance/cashPaymentDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cashPaymentDeleteConfirmDialog</from-outcome>
            <to-view-id>/job/finance/cashPaymentDeleteConfirmDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobDialog</from-outcome>
            <to-view-id>/job/jobDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>labelDialog</from-outcome>
            <to-view-id>/label/labelDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>   
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>legalDocumentDialog</from-outcome>
            <to-view-id>/legal/legalDocumentDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>legalDocumentDeleteConfirmDialog</from-outcome>
            <to-view-id>/legal/legalDocumentDeleteConfirmDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
         
    <managed-bean>
        <managed-bean-name>jobManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.jmts.manager.JobManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>jobSampleManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.jmts.manager.JobSampleManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
       
    <managed-bean>
        <managed-bean-name>jobContractManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.jmts.manager.JobContractManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>jobFinanceManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.jmts.manager.JobFinanceManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>energyLabelManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.mt.manager.EnergyLabelManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>legalDocumentManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.lo.manager.LegalDocumentManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
          
</faces-config>
