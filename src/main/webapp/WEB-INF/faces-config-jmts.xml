    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobGroupingDialog</from-outcome>
            <to-view-id>/job/grouping/jobGroupingDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobStatusAndTrackingDialog</from-outcome>
            <to-view-id>/job/tracking/jobStatusAndTracking.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobReportingDialog</from-outcome>
            <to-view-id>/job/reporting/jobReportingDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>priceListDialog</from-outcome>
            <to-view-id>/job/finance/priceListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
