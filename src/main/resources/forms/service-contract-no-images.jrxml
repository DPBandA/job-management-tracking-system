     ELSE 'No'
   END AS METHODOFDISPOSAL
FROM job_jobsample
	INNER JOIN jobsample ON 
	 job_jobsample.`jobSamples_ID` = jobsample.`ID` 
	INNER JOIN job ON 
	 job_jobsample.`Job_ID` = job.`ID` 
WHERE 
	 job.`ID` = $P{jobSample_jobId}]]>
