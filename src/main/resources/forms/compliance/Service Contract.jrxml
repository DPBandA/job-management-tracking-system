     `jobcostingandpayment` jobcostingandpayment INNER JOIN `job` job ON jobcostingandpayment.`ID` = job.`JOBCOSTINGANDPAYMENT_ID`
     INNER JOIN `jobcostingandpayment_costcomponent` jobcostingandpayment_costcomponent ON jobcostingandpayment.`ID` = jobcostingandpayment_costcomponent.`JobCostingAndPayment_ID`
     INNER JOIN `costcomponent` costcomponent ON jobcostingandpayment_costcomponent.`costComponents_ID` = costcomponent.`ID`
WHERE
     job.`ID` = $P{costComponent_jobId}
ORDER BY
     costcomponent.`ID` ASC]]>
